<div class="container">
  <div class="row col-7 mx-auto">
    <h2 class="text-center mt-5 mb-4">ユーザー一覧</h2>
    
    <table class="table">
      <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= link_to "ユーザーページ", admins_user_path(user.id) %></td>
        <td><%= link_to "予約一覧", reservations_path(user.id) %></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>